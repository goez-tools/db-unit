{
  "name": "goez/db-unit",
  "description": "Database Unit Testing Library based on illuminate/database",
  "authors": [
    {
      "name": "Jace Ju",
      "email": "jaceju@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6",
    "laravel/framework": "^5.4",
    "fzaninotto/faker": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Goez\\DbUnit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "license": "MIT"
}
